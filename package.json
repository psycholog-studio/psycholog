{
  "name": "psycholog",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/psycholog-studio/psycholog.git"
  },
  "scripts": {
    "docs:dev": "yarn workspace @psycholog-studio/docs run dev",
    "docs:export": "yarn workspace @psycholog-studio/docs run export",
    "eslint:check": "eslint .",
    "version": "yarn install && git stage yarn.lock",
    "release:build": "yarn workspaces foreach -ptR --from '@psycholog-studio/{ui,scenes,utils}' run build",
    "release:version": "lerna version --force-publish --conventional-commits",
    "release:publish": "lerna publish from-package --dist-tag latest -y --contents build",
    "husky:prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/is-ci": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-mdx": "^1.17.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-testing-library": "^5.2.1",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "workspaces": [
    "packages/*",
    "lab/*",
    "docs"
  ],
  "packageManager": "yarn@3.1.1"
}
