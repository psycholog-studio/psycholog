{
  "name": "psycholog",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/psycholog-studio/psycholog.git"
  },
  "scripts": {
    "docs:dev": "start-storybook -p 6006",
    "docs:export": "build-storybook -o ./out-docs --quiet",
    "eslint:check": "eslint .",
    "version": "yarn install && git stage yarn.lock",
    "release:build": "yarn workspaces foreach -ptR --from '@psycholog-studio/{ui,scenes,utils}' run build",
