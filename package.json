    "release:publish": "lerna publish from-package --dist-tag latest -y --contents build",
    "husky:prepare": "is-ci || husky install",
    "tool:graph": "yarn nx graph",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
